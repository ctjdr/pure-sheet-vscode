{
    "name": "Pure Sheet",
    "type": "light",
    "semanticHighlighting": true,
    "semanticTokenColors": {
        "keyword.documentation": {
            "foreground": "#4C770F",
            "fontStyle": "bold"
        },
        "interface": {
            "foreground": "#000000"
        },
        "class": {
            "foreground": "#000000",
            "fontStyle": ""
        },
        "class.declaration.constructor": {
            "foreground": "#474747",
            "fontStyle": "bold"
        },
        "namespace": {
            "foreground": "#3a3a3a",
            "bold": false
        },
        "parameter": {
            "foreground": "#6A3E3E"
        },
        "method": {
            "foreground": "#000000",
            "fontStyle": ""
        },
        "method.declaration": {
            "foreground": "#474747",
            "fontStyle": "bold"
        },
        "method.static": {
            "foreground": "#000000",
            "fontStyle": "italic"
        },
        "method.static.declaration": {
            "foreground": "#474747",
            "fontStyle": "bold"
        },
        "function.declaration": {
            "foreground": "#474747",
            "fontStyle": "bold"
        },
        "property": {
            "foreground": "#063FDC"
        },
        "property.static": {
            "italic": true,
            "bold": true
        },
        "variable": {
            "foreground": "#6A3E3E"
        }
    },
